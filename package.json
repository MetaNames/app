{
	"name": "meta-names-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prepare": "yarn run smui-theme-light && yarn run smui-theme-dark",
		"smui-theme-light": "smui-theme compile src/styles/theme/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile src/styles/theme/smui-dark.css -i src/theme/dark",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@arisbh/marqueeck": "^0.5.0",
		"@iconify/svelte": "^4.0.2",
		"@material/typography": "^14.0.0",
		"@playwright/test": "^1.48.1",
		"@smui/banner": "^7.0.0",
		"@smui/button": "^7.0.0",
		"@smui/card": "^7.0.0",
		"@smui/circular-progress": "^7.0.0",
		"@smui/data-table": "^7.0.0",
		"@smui/dialog": "^7.0.0",
		"@smui/form-field": "^7.0.0",
		"@smui/icon-button": "^7.0.0",
		"@smui/linear-progress": "^7.0.0",
		"@smui/list": "^7.0.0",
		"@smui/menu": "^7.0.0",
		"@smui/menu-surface": "^7.0.0",
		"@smui/paper": "^7.0.0",
		"@smui/radio": "^7.0.0",
		"@smui/select": "^7.0.0",
		"@smui/snackbar": "^7.0.0",
		"@smui/tab": "^7.0.0",
		"@smui/tab-bar": "^7.0.0",
		"@smui/textfield": "^7.0.0",
		"@smui/top-app-bar": "^7.0.0",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-vercel": "^5.4.5",
		"@sveltejs/kit": "^2.8.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/siema": "^1.4.11",
		"@typescript-eslint/eslint-plugin": "^8.10.0",
		"@typescript-eslint/parser": "^8.10.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"smui-theme": "^7.0.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.5",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.0",
		"typescript": "^5.6.3",
		"vite": "^5.4.9",
		"vite-plugin-node-polyfills": "^0.22.0",
		"vite-tsconfig-paths": "^5.0.1",
		"vitest": "^2.1.3"
	},
	"type": "module",
	"dependencies": {
		"@ledgerhq/hw-transport-webusb": "^6.27.2",
		"@metanames/sdk": "^6.3.0",
		"@partisiablockchain/abi-client": "^5.48.0",
		"@secata-public/bitmanipulation-ts": "^3.2.0",
		"@sentry/sveltekit": "^8.34.0",
		"@vercel/analytics": "^1.3.1",
		"@vercel/speed-insights": "^1.0.12",
		"chart.js": "^4.4.5",
		"date-fns": "^4.1.0",
		"jdenticon": "^3.3.0",
		"partisia-blockchain-applications-sdk": "^0.1.4",
		"svelte-chartjs": "^3.1.5"
	}
}
