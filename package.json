{
	"name": "meta-names-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prepare": "yarn run smui-theme-light && yarn run smui-theme-dark",
		"smui-theme-light": "smui-theme compile src/styles/theme/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile src/styles/theme/smui-dark.css -i src/theme/dark",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin src --check . && eslint .",
		"format": "prettier --plugin src --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@arisbh/marqueeck": "^0.5.0",
		"@material/typography": "^14.0.0",
		"@playwright/test": "^1.28.1",
		"@smui/banner": "^7.0.0-beta.16",
		"@smui/button": "^7.0.0-beta.14",
		"@smui/card": "^7.0.0-beta.14",
		"@smui/circular-progress": "^7.0.0-beta.15",
		"@smui/data-table": "^7.0.0-beta.15",
		"@smui/dialog": "^7.0.0-beta.15",
		"@smui/icon-button": "^7.0.0-beta.14",
		"@smui/linear-progress": "^7.0.0-beta.15",
		"@smui/list": "^7.0.0-beta.14",
		"@smui/menu": "^7.0.0-beta.14",
		"@smui/menu-surface": "^7.0.0-beta.14",
		"@smui/paper": "^7.0.0-beta.15",
		"@smui/select": "^7.0.0-beta.14",
		"@smui/snackbar": "^7.0.0-beta.15",
		"@smui/tab": "^7.0.0-beta.15",
		"@smui/tab-bar": "^7.0.0-beta.15",
		"@smui/textfield": "^7.0.0-beta.14",
		"@smui/top-app-bar": "^7.0.0-beta.14",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@types/siema": "^1.4.11",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"material-icons": "^1.13.12",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "^7.0.0-beta.14",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-plugin-node-polyfills": "^0.7.0",
		"vite-tsconfig-paths": "^4.3.1",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"@metanames/sdk": "^5.1.1",
		"@sentry/profiling-node": "^7.101.0",
		"@sentry/sveltekit": "^7.101.0",
		"@vercel/analytics": "^1.1.1",
		"@vercel/speed-insights": "^1.0.2",
		"date-fns": "^3.3.1",
		"jdenticon": "^3.2.0",
		"partisia-sdk": "^0.1.1"
	}
}
