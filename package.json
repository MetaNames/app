{
	"name": "meta-names-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prepare": "yarn run smui-theme-light && yarn run smui-theme-dark",
		"smui-theme-light": "smui-theme compile src/styles/theme/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile src/styles/theme/smui-dark.css -i src/theme/dark",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin src --check . && eslint .",
		"format": "prettier --plugin src --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@arisbh/marqueeck": "^0.5.0",
		"@material/typography": "^14.0.0",
		"@playwright/test": "^1.42.1",
		"@smui/banner": "^7.0.0-beta.18",
		"@smui/button": "^7.0.0-beta.18",
		"@smui/card": "^7.0.0-beta.18",
		"@smui/circular-progress": "^7.0.0-beta.18",
		"@smui/data-table": "^7.0.0-beta.18",
		"@smui/dialog": "^7.0.0-beta.18",
		"@smui/form-field": "^7.0.0-beta.18",
		"@smui/icon-button": "^7.0.0-beta.18",
		"@smui/linear-progress": "^7.0.0-beta.18",
		"@smui/list": "^7.0.0-beta.18",
		"@smui/menu": "^7.0.0-beta.18",
		"@smui/menu-surface": "^7.0.0-beta.18",
		"@smui/paper": "^7.0.0-beta.18",
		"@smui/radio": "^7.0.0-beta.18",
		"@smui/select": "^7.0.0-beta.18",
		"@smui/snackbar": "^7.0.0-beta.18",
		"@smui/tab": "^7.0.0-beta.18",
		"@smui/tab-bar": "^7.0.0-beta.18",
		"@smui/textfield": "^7.0.0-beta.18",
		"@smui/top-app-bar": "^7.0.0-beta.18",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-vercel": "^5.2.0",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/siema": "^1.4.11",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"@typescript-eslint/parser": "^7.3.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"material-icons": "^1.13.12",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"smui-theme": "^7.0.0-beta.18",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.8",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.4.1",
		"typescript": "^5.4.3",
		"vite": "^5.2.3",
		"vite-plugin-node-polyfills": "^0.21.0",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.4.0"
	},
	"type": "module",
	"dependencies": {
		"@metanames/sdk": "^6.1.1",
		"@partisiablockchain/abi-client": "^3.30.0",
		"@secata-public/bitmanipulation-ts": "^3.0.7",
		"@sentry/sveltekit": "^7.108.0",
		"@vercel/analytics": "^1.2.2",
		"@vercel/kv": "^1.0.1",
		"@vercel/speed-insights": "^1.0.10",
		"chart.js": "^4.4.2",
		"date-fns": "^3.6.0",
		"jdenticon": "^3.2.0",
		"partisia-sdk": "^0.1.1",
		"svelte-chartjs": "^3.1.5"
	},
	"browser": {
		"crypto": false,
		"stream": false
	}
}
